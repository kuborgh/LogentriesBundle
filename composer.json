{
  "name": "kuborgh/logentries-bundle",
  "type": "symfony-bundle",
  "description": "Bundle to integrate logentries as monolog logger into symfony",
  "keywords": [
    "symfony",
    "logentries",
    "logging",
    "monolog"
  ],
  "homepage": "https://github.com/kuborgh/logentries-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Hannes Giesenow",
      "email": "hgiesenow@kuborgh.de"
    }
  ],
  "require": {
    "symfony/framework-bundle": "~2.3",
    "monolog/monolog": ">=1.4",
    "guzzlehttp/guzzle": "~5.2",
    "jms/serializer": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "symfony/property-access": "~2.3"
  },
  "autoload": {
    "psr-0": {
      "Kuborgh\\LogentriesBundle": ""
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "target-dir": "Kuborgh/LogentriesBundle"
}
